@model IEnumerable<EHealthCard.Models.Insurance>

@{
	ViewData["Title"] = "Index";
}

<h1>Insurances</h1>

<p>
	<a asp-action="Create">Create New</a>
</p>
<p>
	<a asp-action="Search">Search Insurances</a>
</p>
@if (Model.Count() != 0)
{
	<table class="table">
		<thead>
			<tr>
				<th>
					@Html.DisplayNameFor(model => model.Person)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.Comp)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.DateStart)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.DateEnd)
				</th>						
				<th></th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
				<tr>								
					<td>
						@Html.DisplayFor(modelItem => item.Person.PersonId)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Comp.CompId)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.DateStart)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.DateEnd)
					</td>
					<td>
						@Html.ActionLink("Edit", "Edit",  item ) |
						@Html.ActionLink("Details", "Details", item) |
						@Html.ActionLink("Delete", "Delete",  item)
					</td>
				</tr>
			}
		</tbody>
	</table>
}
