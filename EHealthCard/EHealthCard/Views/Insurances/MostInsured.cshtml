@model IEnumerable<EHealthCard.Models.MostInsured>

@{
    ViewData["Title"] = "Table";
}

@if (Model.Count() != 0)
{
    <h3 align="center">Count of Diagnosis Cases in Hospital Over Year</h3>
    <hr />
    <table class="table">
        <thead>
            <tr>
                <th>
                    Person ID
                </th>
                <th>
                    Name
                </th>
                <th>
                    Last Name
                </th>
                <th>
                    Number of Insurances
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.PersonId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LastName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NumberInsured)
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
